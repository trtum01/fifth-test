<center><h1>Home</h1>
<p><%= link_to 'New', new_article_path %></p>
<table>
  <tr>
    <th>Name</th>
    <th>Lastname</th>
    <th>Age</th>
  </tr>
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.name %></td>
      <td><%= article.lastname %></td>
      <td><%= article.age %></td>
      <td><%= link_to 'Show', article_path(article) %></td>
      <td><%= link_to 'Edit', edit_article_path(article) %></td>
      <td><%= link_to 'Delete', article_path(article), method: :delete, data: { confirm: 'Are you sure Yep?' }%></td>
    </tr>
  <% end %>
</table></center>
